int kirmizi=12;
int mavi=13;
void setup(){pinMode(kirmizi,OUTPUT);
pinMode(mavi,OUTPUT);
}
void loop(){
  for (int i =0;i<5;i++){digitalWrite(kirmizi,1);
  delay(100);
  digitalWrite(kirmizi,0);
  delay(50);}
  for(int i =0;i<5;i++){
    digitalWrite(mavi,1);
    delay(100);
    digitalWrite(mavi,0);
    delay(50);}
  }
